#include </home/duciito/.termcolors/skater> 

Xft.dpi: 96
Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

! Colors 
*.background:   bg  
*.foreground:   fg
*.cursorColor: wht  
*.color0:      blk  
*.color8:      bblk  
*.color1:      red  
*.color9:      bred  
*.color2:      grn  
*.color10:     bgrn  
*.color3:      ylw  
*.color11:     bylw  
*.color4:      blu  
*.color12:     bblu  
*.color5:      mag  
*.color13:     bmag  
*.color6:      cyn  
*.color14:     bcyn  
*.color7:      wht  
*.color15:     bwht  

rofi.color-enabled: true
rofi.color-normal: blu, bg, blu,  mag,   bg
rofi.color-active: blu, bg, blu,  mag,   bg
rofi.color-urgent: blu, bg, blu,  bg,  bg
rofi.color-window: blu, blu, blu
